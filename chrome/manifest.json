{
  "name": "Printful Variant ID",
  "description": "Display hidden variant IDs on printful",
  "version": "1.0",
  "icons": {
    "128": "icon128.png"
  },
  "permissions": [
    "tabs",
    "https://www.theprintful.com/dashboard/sync*"
  ],
  "content_scripts": [ {
    "matches": [
      "https://www.theprintful.com/dashboard/sync*"
      ],
    "js": ["content.js"],
    "run_at": "document_end"
  }],

  "manifest_version": 2
}
